<p id="notice"><%= notice %></p>

<h1>Admin Panels</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_panels.each do |admin_panel| %>
      <tr>
        <td><%= link_to 'Show', admin_panel %></td>
        <td><%= link_to 'Edit', edit_admin_panel_path(admin_panel) %></td>
        <td><%= link_to 'Destroy', admin_panel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Panel', new_admin_panel_path %>
